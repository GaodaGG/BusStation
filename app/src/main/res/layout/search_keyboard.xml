<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:parentTag="com.google.android.material.card.MaterialCardView">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/item_search_keyboard_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/item_search_keyboard_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="1"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="2"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/item_search_keyboard_1"
            app:layout_constraintStart_toEndOf="@id/item_search_keyboard_1"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="3"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/item_search_keyboard_1"
            app:layout_constraintStart_toEndOf="@id/item_search_keyboard_2"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="4"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/item_search_keyboard_1"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="5"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/item_search_keyboard_4"
            app:layout_constraintStart_toEndOf="@id/item_search_keyboard_4"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="6"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/item_search_keyboard_4"
            app:layout_constraintStart_toEndOf="@id/item_search_keyboard_5"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="7"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/item_search_keyboard_4"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="8"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/item_search_keyboard_7"
            app:layout_constraintStart_toEndOf="@id/item_search_keyboard_7"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="9"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/item_search_keyboard_7"
            app:layout_constraintStart_toEndOf="@id/item_search_keyboard_8"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_delete"
            style="@style/Widget.Material3.Button.IconButton.Filled"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:contentDescription="@string/search_keyboard_delete"
            android:tag="delete"
            app:icon="@drawable/ic_keyboard_delete"
            app:iconGravity="textStart"
            app:iconSize="24dp"
            app:layout_constraintEnd_toEndOf="@+id/item_search_keyboard_7"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/item_search_keyboard_7"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/item_search_keyboard_8"
            app:layout_constraintStart_toStartOf="@id/item_search_keyboard_8"
            app:layout_constraintTop_toTopOf="@id/item_search_keyboard_delete"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/item_search_keyboard_backspace"
            style="@style/Widget.Material3.Button.IconButton.Filled"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:contentDescription="@string/search_keyboard_backspace"
            android:tag="backspace"
            app:icon="@drawable/ic_keyboard_backspace"
            app:iconGravity="textStart"
            app:iconSize="24dp"
            app:layout_constraintBaseline_toBaselineOf="@id/item_search_keyboard_delete"
            app:layout_constraintEnd_toEndOf="@+id/item_search_keyboard_9"
            app:layout_constraintStart_toStartOf="@id/item_search_keyboard_9"
            tools:ignore="HardcodedText" />

        <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:scrollbars="none"
            app:layout_constraintBottom_toBottomOf="@id/item_search_keyboard_backspace"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/item_search_keyboard_3"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck">

            <LinearLayout
                android:id="@+id/item_search_keyboard_scrollview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" />

        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>
